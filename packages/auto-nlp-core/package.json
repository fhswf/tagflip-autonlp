{
  "name": "auto-nlp-core",
  "version": "0.1.0",
  "description": "Automated training and deployment of NLP Models.",
  "author": "Timo Neuhaus",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
