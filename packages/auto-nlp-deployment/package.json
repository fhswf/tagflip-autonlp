{
  "name": "auto-nlp-deployment",
  "version": "0.1.0",
  "description": "Adapting service for deploying trainings and models.",
  "author": "Timo Neuhaus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/fhswf/tagflip-autonlp.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "install": "run-script-os",
    "install:linux": "python -m venv ./venv && /bin/bash -c 'export PIPENV_IGNORE_VIRTUALENVS=1 && source ./venv/bin/activate && pipenv install'",
    "start": "run-script-os",
    "start:linux": "/bin/bash -c 'source ./venv/bin/activate && pipenv run uvicorn --app-dir ./src --port 8085 main:app'",
    "start-dev": "run-script-os",
    "start-dev:linux": "/bin/bash -c 'source ./venv/bin/activate && pipenv run uvicorn --app-dir ./src --port 8085 --reload main:app'"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "run-script-os": "^1.1.6",
    "semantic-release": "^19.0.2",
    "semantic-release-monorepo": "^7.0.5"
  }
}
